<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.layout.StackPane?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="600.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true">
            <HBox.margin>
                <Insets right="10" bottom="10" left="10"/>
            </HBox.margin>
            <style>
                -fx-background-color: #e7e7e7;
                -fx-background-radius: 15;
                -fx-padding: 10;
            </style>
         </Label>
        <StackPane fx:id="imageContainer">
            <ImageView fx:id="pictureDisplayer" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true" />
        </StackPane>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
